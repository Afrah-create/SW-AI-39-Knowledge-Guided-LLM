{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "ignore": [
    "models/",
    "quick_fine_tuned_fast/"
  ]
}

